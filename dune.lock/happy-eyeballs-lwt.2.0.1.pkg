(version 2.0.1)

(build
 (all_platforms
  ((action
    (progn
     (when %{pkg-self:dev} (run dune subst))
     (run dune build -p %{pkg-self:name} -j %{jobs}))))))

(depends
 (all_platforms
  (ocaml
   dune
   happy-eyeballs
   cmdliner
   duration
   dns
   domain-name
   ipaddr
   fmt
   logs
   lwt
   mtime)))

