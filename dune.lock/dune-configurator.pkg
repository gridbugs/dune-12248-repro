(version 3.20.0)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run rm -rf vendor/csexp)
  (run rm -rf vendor/pp)
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends dune ocaml base-unix csexp)

(source
 (fetch
  (url
   https://github.com/ocaml/dune/releases/download/3.20.0/dune-3.20.0.tbz)
  (checksum
   sha256=767999da81e528484139ecfe927288ffe521ac2467462a40d6a760ed789ba086)))
